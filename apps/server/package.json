{
  "name": "@aics/server",
  "version": "0.1.0",
  "private": true,
  "description": "AICS Backend - NestJS API Server",
  "scripts": {
    "dev": "dotenv -e ../../.env -- ts-node -r tsconfig-paths/register src/main.ts",
    "build": "nest build",
    "start": "dotenv -e ../../.env -- node dist/main",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "prisma:generate": "dotenv -e ../../.env -- prisma generate",
    "prisma:migrate": "dotenv -e ../../.env -- prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -e ../../.env -- prisma migrate dev",
    "prisma:seed": "dotenv -e ../../.env -- ts-node prisma/seed.ts",
    "prisma:studio": "dotenv -e ../../.env -- prisma studio"
  },
  "dependencies": {
    "@aics/shared": "workspace:*",
    "@nestjs/bullmq": "^10.2.0",
    "@nestjs/common": "^10.4.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^8.1.0",
    "@prisma/client": "^6.0.0",
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "imap-simple": "^5.1.0",
    "ioredis": "^5.4.0",
    "mailparser": "^3.7.0",
    "mammoth": "^1.8.0",
    "nodemailer": "^6.9.0",
    "openai": "^4.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.0",
    "@nestjs/schematics": "^10.2.0",
    "@types/bcryptjs": "^2.4.0",
    "@types/express": "^5.0.0",
    "@types/imap-simple": "^4.2.0",
    "@types/mailparser": "^3.4.0",
    "@types/node": "^22.0.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^4.0.0",
    "dotenv-cli": "^11.0.0",
    "prisma": "^6.0.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.0"
  }
}
